{
	"opts": {
		"destination": "docs/js",
		"pedantic": true,
		"readme": "readme.md",
		"recurse": true,
		"template": "node_modules/jsdoc-wmf-theme"
	},
	"plugins": [
		"node_modules/jsdoc-wmf-theme/plugins/allow-dots-in-modules",
		"plugins/markdown",
		"node_modules/jsdoc-wmf-theme/plugins/summarize",
		"node_modules/jsdoc-wmf-theme/plugins/betterlinks",
		"node_modules/jsdoc-wmf-theme/plugins/externals"
	],
	"source": {
		"include": [ "modules" ],
		"exclude": [ ]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		},
		"wmf": {
			"maintitle": "CollaborationKit",
			"repository": "https://gerrit.wikimedia.org/g/mediawiki/extensions/CollaborationKit",
			"siteMap": {
				"sections": true
			},
			"prefixMap": {
				"OO.ui.": "https://doc.wikimedia.org/oojs-ui/master/js/{type}.html"
			},
			"linkMap": {
				"Array": "https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Array",
				"jQuery": "https://api.jquery.com/jQuery/",
				"jQuery.Promise": "https://api.jquery.com/Types/#Promise",
				"mw": "https://doc.wikimedia.org/mediawiki-core/master/js/mw.html",
				"OO": "https://doc.wikimedia.org/oojs/master/OO.html"
			}
		}
	}
}
